generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id               String    @id @default(cuid())
  email            String    @unique
  password         String
  resetToken       String?
  resetTokenExpiry DateTime?
  // Add other fields as needed
}
